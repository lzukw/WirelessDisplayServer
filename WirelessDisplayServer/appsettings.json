{
  "Windows" :
  {
    "shell" :"cmd.exe",
    "shell_Args_Template" : "/c \"%SCRIPT %ARGS\"",
    "Start_Streaming_Script_Path": "..\\Scripts\\Windows\\startStreamingSink.bat",
    "Start_Streaming_Script_Args_Template": "%STREAMING_TYPE %PORT_NO",
    "Manage_Screen_Resolutions_Script_Path": "..\\Scripts\\Windows\\manageScreenResolutions.bat",
    "Manage_Screen_Resolutions_Script_Args_Template": "%ACTION %RESOLUTION"
  },

  "Linux" :
  {
    "shell" :"bash",
    "shell_Args_Template" : "%SCRIPT %ARGS",
    "Start_Streaming_Script_Path": "../Scripts/Linux/startStreamingSink.sh",
    "Start_Streaming_Script_Args_Template": "%STREAMING_TYPE %PORT_NO",
    "Manage_Screen_Resolutions_Script_Path": "../Scripts/Linux/manageScreenResolutions.sh",
    "Manage_Screen_Resolutions_Script_Args_Template": "%ACTION %RESOLUTION"
  },

  "macOS":
  {
    "shell" :"???",
    "shell_Args_Template" : "???",
    "Start_Streaming_Script_Path": "../Scripts/macOS/startStreamingSink.???",
    "Start_Streaming_Script_Args_Template": "%STREAMING_TYPE %PORT_NO",
    "Manage_Screen_Resolutions_Script_Path": "../Scripts/macOS/manageScreenResolutions.???",
    "Manage_Screen_Resolutions_Script_Args_Template": "%ACTION %RESOLUTION"
  },

  "urls": "http://*:80",

  "Logging": {
    "LogLevel": {
